# Tells Terraform to use AWS and which region to talk to.
# Credentials will be taken from your local AWS config:


terraform {
  required_version = ">= 1.5.0"

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
  }
}

provider "aws" {
  region = "us-east-1"
}
